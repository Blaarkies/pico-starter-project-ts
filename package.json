    "esbuild": "^0.20.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@kaluma/cli": "^1.4.0",
    "kefir": "^3.8.8"
  }
}
