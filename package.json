{
  "name": "pico-ts-getting-started",
  "version": "0.0.1",
  "description": "Starter project for programs running on the RP2040 chip",
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "build": "node scripts/esbuild.config.js",
    "build-minify": "npm run build -- --minify",
